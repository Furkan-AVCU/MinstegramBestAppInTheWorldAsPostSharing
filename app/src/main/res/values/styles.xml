<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:actionOverflowButtonStyle">@style/MyThemeForMenuIcon</item>
        <item name="android:listDivider">@color/black</item>


    </style>
    
    <style name="MyThemeForMenuIcon" parent="android:style/Widget.Holo.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_baseline_more_vert_24</item>
    </style>

    <style name="MyBackbuttonIcon" parent="AppTheme">
        <item name="android:homeAsUpIndicator">@drawable/ic_baseline_arrow_back_24</item>
    </style>

</resources>